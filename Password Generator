import random

caracteres = "+-/*!&$#?=@abcdefghijklnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890"

longitud = int(input("introduce la cantidad de caracteres que quieres en tu contrase√±a:"))

password = ""

for i in range(longitud):
    p = random.choice(caracteres)
    password += p
    print(password)
